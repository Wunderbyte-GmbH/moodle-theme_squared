<div class="panel-group single-row" id="accordion" role="tablist" aria-multiselectable="true">
    {{#blocks}}
        <div class="{{class}} pairheader left">
            <div role="button" data-toggle="collapse" data-parent="#accordion" 
                        href="#collapse{{blockinstanceid}}" aria-expanded="true" aria-controls="collapseOne" class="{{shape}} blockheader over-hover-to-bottom {{name}}">
                <span {{#blockinstanceid}} id="instance-{{blockinstanceid}}-header"{{/blockinstanceid}} >
                    <span class="blockhead title">
                        <div class="courseblock-icon"></div>
                        <span class="blocktitle">
                        {{{title}}}
                        </span>
                    </span>
                </span>
            </div>
        </div>    

        {{#blockinstanceid}}
        <div {{#atts}} {{key}} = "{{value}}" {{/atts}}>
            <div id="collapse{{blockinstanceid}}" class="panel-collapse collapse {{#open}}in{{/open}}" role="tabpanel" aria-labelledby="{{arialabel}}">
                <div class="panel-heading">
                    <div class="courseblock-icon"></div>
                    {{{header}}}
                 </div>
                <div class="panel-body p-0 m-0 block">
                    {{{content}}}
                </div>
            </div>
        </div>
        <a href="#sb-{{skipid}}" class="skip-block"> {{#str}} skipa, access, {{skiptitle}} {{/str}} </a>
        {{/blockinstanceid}}
        {{#movetarget}}
            {{{movetarget}}}
        {{/movetarget}}
    {{/blocks}}
</div>