<header role="banner" class="navbar navbar-expand-lg navbar-light bg-light moodle-has-zindex position-{{#navpositionfixed}}fixed{{/navpositionfixed}}{{^navpositionfixed}}static{{/navpositionfixed}}">
    {{#primarymoremenu}}
        <div class="primary-navigation">
            {{> core/moremenu}}
        </div>
    {{/primarymoremenu}}
    <ul class="nav navbar-nav mr-auto sq-hidden-md-down">
        <!-- custom_menu -->
        {{{ output.custom_menu }}}
        <!-- page_heading_menu -->
        {{{ output.page_heading_menu }}}
    </ul>

    <ul class="nav nav-nowrap ml-auto">
        {{#searchbox}}
        <!-- search_box -->
        <li class="nav-item sq-hidden-md-down">
            {{{ searchbox }}}
        </li>
        {{/searchbox}}

        {{#langmenu}}
        <li class="nav-item">
            {{> theme_boost/language_menu }}
            <div class="divider border-left h-75 align-self-center mx-1"></div>
        </li>
        {{/langmenu}}

        <!-- navbar_settings_menu_output -->
        <li class="nav-item">
            {{{ output.navbar_settings_menu }}}
        </li>

        <!-- navbar_plugin_output -->
        <li class="nav-item d-flex">
            {{{ output.navbar_plugin_output }}}
        </li>

        {{#usermenu}}
        <!-- user_menu -->
        <li class="nav-item">
            {{> core/user_menu }}
        </li>
        {{/usermenu}}
    </ul>
    {{{ output.edit_switch }}}
</header>